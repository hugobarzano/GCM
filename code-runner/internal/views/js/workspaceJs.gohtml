{{define "js"}}
    <script>
        $(document).ready(function() {
            setInterval(function(){
                $.ajax({
                    url: "/getWs",
                    type: "get",
                    data: {owner: "{{ .Owner}}"},
                    cache: false,
                    success: function (response) {
                        $('#apps').replaceWith(response)
                        console.log(response)
                    },
                    error: function (xhr) {
                    }
                });}, 5000);
        });
    </script>
{{end}}